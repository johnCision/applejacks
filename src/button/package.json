{
	"name": "@johncision/applejacks-button",
	"version": "1.0.0",
	"description": "UI Framework",
	"type": "module",
	"exports": {
		".": "./src/button.js"
	},
	"files": [
		"src/**/*.js"
	],
	"scripts": {
		"lint": "eslint --quiet .",
		"test": "jest",
		"coverage": "c8 --lines 2 --functions 0 --branches 2 --check-coverage --all npm run test -- --no-parallel --reporter min"
	},
	"eslintConfig": {
		"extends": [
			"../../package.json"
		]
	},
	"jest": {
		"verbose": true,
		"collectCoverage": true,
		"coverageDirectory": "coverage",
		"coverageProvider": "v8",
		"moduleFileExtensions": [
			"js"
		],
		"testMatch": [
			"**/test/*.js"
		],
		"transform": {}
	},
	"mocha": {
		"spec": [
			"spec/*.spec.js"
		],
		"watch": false,
		"sort": false,
		"forbitOnly": true,
		"check-leaks": true
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/johnCision/applejacks.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/johnCision/applejacks/issues"
	},
	"homepage": "https://github.com/johnCision/applejacks#readme",
	"devDependencies": {
		"@jest/globals": "^27.3.1",
		"@johncision/eslint-config": "^3.0.9",
		"c8": "^7.10.0",
		"chai": "^4.3.4",
		"eslint": "^8.0.1",
		"eslint-plugin-fp": "^2.3.0",
		"eslint-plugin-immutable": "^1.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-no-loops": "^0.3.0",
		"eslint-plugin-security": "^1.4.0",
		"eslint-plugin-spellcheck": "^0.0.19",
		"jest": "^27.3.1",
		"mocha": "^9.1.3"
	}
}